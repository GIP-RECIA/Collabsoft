{
  "name": "collabsoft",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "npx playwright install && playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src/main/webapp/src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format:check": "prettier --check src/main/webapp/src/",
    "format": "prettier --write src/main/webapp/src/",
    "clean": "find . -name node_modules -type d -prune -o -name cache -type d -o -name dist -type d -o -name playwright-report -type d -o -name test-results -type d | xargs rm -rf",
    "prepare": "husky install",
    "predev": "dotenv-checker --schema .env.example --env .env.local",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@gip-recia/tldraw-webcomponent": "^3.6.0",
    "@gip-recia/wisemapping-webcomponent": "^0.1.0",
    "@vueuse/core": "^10.11.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.30",
    "vue-i18n": "9.13.1",
    "vue-router": "^4.4.0",
    "vuetify": "^3.6.10"
  },
  "packageManager": "yarn@4.3.1"
}
